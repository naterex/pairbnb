<div class="row">
  <div class="col-sm-8 col-sm-offset-2 col-md-7 col-md-offset-3 col-lg-6 col-lg-offset-3" id="listing">
  <center>
    <h2><%= t(".edit_listing") %></h2>
  </center>

  <%= form_for @listing do |form| %>
    <%= render partial: '/listings/form', object: form %>

    <%= form.submit class: "btn btn-success" %>

    <div class="pull-right">
        <%= link_to t(".cancel"), listing_path, class: "btn btn-danger" %>
    </div>

  <% end %>

  </div>
</div>
