<div class="row">
  <center>
    <h2><%= t(".my_listings") %></h2>
    <div class="col-sm-11 col-sm-offset-1 col-md-10 col-md-offset-2 col-lg-9 col-lg-offset-2">
      <% @listings.each do |listing| %>
        <div class="col-sm-6 col-md-5 col-lg-4" id="my_listings">
          <h3>
            <%= link_to listing.title, listing_path(listing) %>
          </h3>
            <%= image_tag listing.photos[0].url(:thumb).to_s, class: "img-rounded img-responsive" %>
        </div>
      <% end %>
    </div>
  </center>
</div>
